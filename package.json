{
  "name": "dw-cli",
  "version": "3.0.1",
  "description": "",
  "exports": "./cli.js",
  "type": "module",
  "bin": {
    "dw": "cli.js"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "test": "eslint .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint . --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzwallace/dw-cli.git"
  },
  "keywords": [
    "demandware",
    "dw-cli",
    "demandware cli",
    "demandware command-line interface",
    "salesforce commerce cloud"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mzwallace/dw-cli/issues"
  },
  "homepage": "https://github.com/mzwallace/dw-cli#readme",
  "dependencies": {
    "archiver": "5.3.0",
    "bluebird": "3.7.2",
    "chalk": "5.0.0",
    "chokidar": "3.5.2",
    "cwait": "1.1.2",
    "debug": "4.3.3",
    "dotenv": "10.0.0",
    "follow-redirects": "1.14.7",
    "fs-extra": "10.0.0",
    "globby": "12.0.2",
    "got": "12.0.0",
    "lodash": "4.17.21",
    "lodash-es": "4.17.21",
    "node-notifier": "10.0.0",
    "ora": "6.0.1",
    "p-retry": "5.0.0",
    "xml2js": "0.4.23",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "39.0.0",
    "husky": "7.0.4",
    "is-core-module": "2.8.0",
    "lint-staged": "12.1.4",
    "prettier": "2.5.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:unicorn/recommended",
      "plugin:prettier/recommended"
    ],
    "env": {
      "es6": true,
      "node": true,
      "browser": false
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "bracketSpacing": false
        }
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "no-console": 0,
      "unicorn/no-process-exit": 0
    }
  }
}
